{
    "name": "homepage",
    "version": "0.0.1",
    "description": "Russell McWhaeâ€™s portfolio",
    "engines": {
        "node": "22.x"
    },
    "scripts": {
        "dev": "vite dev --port 4000 --host",
        "build": "rm -rf .vercel_build_output && vite build",
        "preview": "vite preview --port 4000",
        "kill": "rm -rf node_modules",
        "lint": "prettier --check . && eslint .",
        "format": "prettier --write .",
        "test": "playwright test",
        "test:ui": "playwright test --ui",
        "test:headed": "playwright test --headed",
        "test:debug": "playwright test --debug",
        "postinstall": "if [ \"$VERCEL\" = \"1\" ]; then pnpm exec npm rebuild sharp --platform=linux --arch=x64 --ignore-scripts=false; fi"
    },
    "devDependencies": {
        "@eslint/js": "^9.37.0",
        "@playwright/test": "^1.55.1",
        "@sveltejs/adapter-vercel": "^5.10.3",
        "@sveltejs/kit": "^2.43.8",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@types/node": "^22.18.8",
        "@types/unist": "^3.0.3",
        "@typescript-eslint/eslint-plugin": "^8.45.0",
        "@typescript-eslint/parser": "^8.45.0",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-svelte": "^3.12.4",
        "imagekit": "^6.0.0",
        "jiti": "^2.6.1",
        "mdsvex": "^0.12.6",
        "photoswipe": "^5.4.4",
        "prettier": "~3.6.2",
        "prettier-plugin-svelte": "^3.4.0",
        "rehype-autolink-headings": "^7.1.0",
        "rehype-external-links": "^3.0.0",
        "rehype-slug": "^6.0.0",
        "remark-gfm": "^4.0.1",
        "sass": "^1.93.2",
        "svelte": "^5.39.8",
        "svelte-gallery": "^1.3.5",
        "svelte-icons": "^2.1.0",
        "svelte-preprocess": "^6.0.3",
        "svimg": "^4.0.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.45.0",
        "vite": "^7.1.9"
    },
    "type": "module",
    "dependencies": {
        "@vercel/analytics": "^1.5.0",
        "@vercel/speed-insights": "^1.2.0",
        "dateformat": "^5.0.3",
        "imagekit-javascript": "^4.0.1",
        "reading-time": "^1.5.0",
        "sharp": "^0.32.6",
        "unist-util-visit": "^5.0.0"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@vercel/speed-insights",
            "svelte-preprocess"
        ]
    }
}
